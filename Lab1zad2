#include <stdio.h>

int main()
{
	int a, b, c;
	printf("Input number for a,b,c\n");
	printf("a=");
	scanf("%d", &a);
	printf("b=");
	scanf("%d", &b);
	printf("c=");
	scanf("%d", &c);
	if (a == -b)
		printf("Opposite numbers");
	else if (a == -c)
		printf("Opposite numbers");
	else if (b == -c)
		printf("Opposite numbers");
	else
		printf("Not opposite numbers");
	return 0;

}
